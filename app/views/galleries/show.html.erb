<div class="container">

  <button class="btn btn-default" type="submit"><%= link_to 'View All Galleries', galleries_path %></button>
  

  <h1><%= @gallery.name %></h1>
  <%= @gallery.address %><br>
  <%= @gallery.phone %><br>
  <%= @gallery.hours %><br>
  <br>
  <em><%= @gallery.medium %></em><br>
  <br>
  <%= @gallery.website %><br>
  <hr>
  <%= markdown(@gallery.description) %><br>
  

</div>